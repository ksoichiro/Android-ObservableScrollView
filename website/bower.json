{
  "name": "Android-ObservableScrollView",
  "description": "bower components for Android-ObservableScrollView",
  "dependencies": {
    "jquery": "1.11.2",
    "bootstrap": "3.3.4",
    "components-font-awesome": "4.3.0",
    "highlightjs": "8.5.0",
    "respond-minmax": "1.4.2",
    "html5shiv": "3.7.2"
  },
  "overrides": {
    "jquery": {
      "main": "dist/*.min.*"
    },
    "bootstrap": {
      "main": ["dist/css/*.min.css", "dist/js/*.min.js", "dist/fonts/*"]
    },
    "components-font-awesome": {
      "main": ["css/*.min.css", "fonts/*"]
    },
    "highlightjs": {
      "main": ["*.js", "**/*.css"]
    },
    "respond-minmax": {
      "main": "dest/*.min.js"
    },
    "html5shiv": {
      "main": "dist/*.min.js"
    }
  }
}
